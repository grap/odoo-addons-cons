<openerp><data>

    <record id="view_invoice_commission_wizard_form" model="ir.ui.view">
        <field name="model">invoice.commission.wizard</field>
        <field name="arch" type="xml">
            <form string="Make Commissions Invoice" version="7.0">
                <group string="Settings">
                    <field name="consignor_partner_id"
                        on_change="on_change_consignor_partner_id_period_id(consignor_partner_id, period_id)"/>
                    <field name="period_id"
                        on_change="on_change_consignor_partner_id_period_id(consignor_partner_id, period_id)"/>
                    <field name="line_qty"/>
                </group>
                <footer>
                    <button icon="gtk-cancel" special="cancel" string="Cancel"/>
                    <button name="invoice_commission" type="object" string="Confirm" class="oe_highlight"/>
               </footer>
            </form>
        </field>
    </record>

</data></openerp>
